terminal







<table class="table table-bordered">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Почта</th>
      <th scope="col">Имя пользователя</th>
      <th scope="col">Уровень доступа</th>
    </tr>
  </thead>
  <%@users.each do |user|%>
  <tbody>
    <tr>
      <th scope="row"><%=user.id%></th>
      <td><%=user.email%></td>
      <td><%=user.username%></td>
      <td><%=user.level%></td>
    </tr>
  </tbody>
  <%end%>
</table>



