<!-- Post Content -->



    <article>
      <div class="container">
        <div class="row">
          <div class="col-lg-10 col-md-12 mx-auto">
          	
            

            <h4 class="text-left">
              <%=@post.content.html_safe%></h4><br/>
        
              

